<?xml version=”1.0” encoding=”UTF-8”?>
<!DOCTYPE robot SYSTEM "robot.dtd">
<robot>

    <chassis type="TANK"
             wheelDiameter="4.0"
             wheelBase="13.75"
             track="15.0"
             maxVelocity="36.0"
             maxAngularVelocity="12.0"
             maxAcceleration="12.0"
             maxAngularAcceleration="6.0">

        <motor usage="DIFFERENTIAL_LEFT_MAIN"
               canId="12"
               pdpID="12"
               type="TALONSRX"
